                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" VerticalAlignment="Center" Margin="12,0,8,0" Grid.Row="10" HorizontalAlignment="Center"><Run Text="Recommended S"/><Run Text="pacing"/></TextBlock>
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" VerticalAlignment="Center" Margin="11,0" Grid.Row="10" Grid.Column="3" HorizontalAlignment="Center"><Run Text="Use Recommended "/><Run Text="Spacing"/></TextBlock>
                        <CheckBox x:Name="SpacingCheck" Content="" Grid.Column="3" HorizontalAlignment="Center" Margin="0,0,0,5" Grid.Row="11" VerticalAlignment="Center" VerticalContentAlignment="Center" Checked="SpacingCheck_Checked"/>
                    </Grid>
                    <Grid Height="50" Margin="0,10,0,0" Grid.Row="2" VerticalAlignment="Top" Grid.ColumnSpan="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="100*"/>
                            <ColumnDefinition Width="100*"/>
                            <ColumnDefinition Width="100*"/>

                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="25"/>
                            <RowDefinition Height="35*"/>
                        </Grid.RowDefinitions>
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" VerticalAlignment="Center" Margin="0" HorizontalAlignment="Center" Grid.Column="1"><Run Text="Outline Color"/></TextBlock>
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" VerticalAlignment="Center" Margin="0" HorizontalAlignment="Center"><Run Text="Panel"/><Run Text=" Color"/></TextBlock>
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="2" Margin="0" Width="100"><Run Text="Exclusion "/><Run Text="Color"/></TextBlock>
                        <ComboBox x:Name="panelColorList1" HorizontalAlignment="Center" Margin="38,3,10,0" VerticalAlignment="Top" ItemsSource="{Binding}" Grid.Row="1" MinWidth="100" Width="127" SelectionChanged="PanelColorList1_SelectionChanged"/>
                        <ComboBox x:Name="outlineColorList1" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Top" ItemsSource="{Binding}" Grid.Row="1" MinWidth="100" Width="100" Grid.Column="1" SelectionChanged="OutlineColorList1_SelectionChanged"/>
                        <ComboBox x:Name="shadingColorList1" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Top" ItemsSource="{Binding}" Grid.Row="1" MinWidth="100" Width="100" Grid.Column="2" SelectionChanged="ShadingColorList1_SelectionChanged"/>
                    </Grid>
                </Grid>

            </TabItem>
            <TabItem x:Name="tabbya" Header="GLOBAL SETTINGS" Width="100">
