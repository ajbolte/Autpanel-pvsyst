                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" Grid.Row="8" Grid.Column="3" VerticalAlignment="Center" Margin="10,0,11,0" HorizontalAlignment="Center" Text="Array Pitch (degrees)"/>
                        <TextBox x:Name="RoofPitch" TextWrapping="Wrap" Text="10" Width="100" RenderTransformOrigin="0.5,-0.238" TextAlignment="Center" Grid.Row="9" Margin="0,0,0,5" TextChanged="RoofPitch_TextChanged" HorizontalAlignment="Center" VerticalAlignment="Center"/>
