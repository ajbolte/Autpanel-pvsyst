                    <Grid.RowDefinitions>
                        <RowDefinition Height="34"/>

                        <RowDefinition Height="343"/>
                        <RowDefinition Height="80*"/>
                    </Grid.RowDefinitions>
                    <ToolBarPanel HorizontalAlignment="Left" Height="100" Margin="0,14,0,0" VerticalAlignment="Top" Width="100" Grid.RowSpan="2" Grid.Column="2"/>
                    <TextBlock HorizontalAlignment="Center" Margin="0,0,9,0" TextWrapping="Wrap" VerticalAlignment="Top" TextAlignment="Center" FontSize="24" RenderTransformOrigin="0.759,1.795" Height="32"><Run Text="Global "/><Run Text="Array"/><Run Text=" "/><Run Text="Settings"/><Run Text=" "/></TextBlock>
