                        <TextBox x:Name="GapDown" TextWrapping="Wrap" Text="0" Width="100" RenderTransformOrigin="0.5,-0.238" TextAlignment="Center" Grid.Row="3" TextChanged="GapDown_TextChanged" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,5"/>
                        <TextBox x:Name="PanelsDown" TextWrapping="Wrap" Text="0" Width="100" RenderTransformOrigin="0.5,-0.238" TextAlignment="Center" Grid.Row="5" TextChanged="PanelsDown_TextChanged" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,5"/>
                        <TextBox x:Name="PanelsAcross" TextWrapping="Wrap" Text="0" Width="100" RenderTransformOrigin="0.5,-0.238" TextAlignment="Center" Grid.Row="5" TextChanged="PanelsAcross_TextChanged" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,5"/>
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" Grid.Row="2" Grid.ColumnSpan="2" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center"><Run Text="Gap Between Panels "/><Run Text="Down"/><Run Text=" (mm"/><Run Text=")"/></TextBlock>
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" Grid.Row="6" VerticalAlignment="Center" HorizontalAlignment="Center"><Run Text="Walkway Across Siz"/><Run Text="e (mm)"/></TextBlock>
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" Grid.Row="6" Grid.Column="3" VerticalAlignment="Center" HorizontalAlignment="Center"><Run Text="Walkway "/><Run Text="Down"/><Run Text=" Siz"/><Run Text="e (mm)"/></TextBlock>
                        <TextBox x:Name="WalkwaySizeAcross" TextWrapping="Wrap" Text="0" Width="100" RenderTransformOrigin="0.5,-0.238" TextAlignment="Center" Grid.Row="7" TextChanged="WalkwaySizeAcross_TextChanged" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,5"/>
                        <TextBox x:Name="WalkwaySizeDown" TextWrapping="Wrap" Text="0" Width="100" RenderTransformOrigin="0.5,-0.238" TextAlignment="Center" Grid.Row="7" Grid.Column="3" TextChanged="WalkwaySizeDown_TextChanged" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,5"/>
