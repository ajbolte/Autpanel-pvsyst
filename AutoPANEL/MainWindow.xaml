                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" Grid.Column="3" VerticalAlignment="Center" HorizontalAlignment="Center"><Run Text="Panel"/><Run Text=" "/><Run Text="Direction"/><Run Text=" (0=North)"/></TextBlock>
                        <TextBox x:Name="azimuth" TextWrapping="Wrap" Text="0" Width="100" RenderTransformOrigin="0.5,-0.238" TextAlignment="Center" TextChanged="Azimuth_TextChanged" Grid.Column="3" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,5"/>
                        <TextBox x:Name="GapAcross" TextWrapping="Wrap" Text="0" Width="100" RenderTransformOrigin="0.5,-0.238" TextAlignment="Center" Grid.Row="3" TextChanged="GapAcross_TextChanged" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,5"/>
