                        <CheckBox x:Name="SpacingCheckGlobal" Content="" Grid.Column="3" HorizontalAlignment="Center" Margin="0,0,0,5" Grid.Row="11" VerticalAlignment="Center" VerticalContentAlignment="Center"/>
                    </Grid>
                    <Grid Height="50" Margin="0,10,0,0" Grid.Row="2" VerticalAlignment="Top" Grid.ColumnSpan="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="100*"/>
                            <ColumnDefinition Width="100*"/>
                            <ColumnDefinition Width="100*"/>

                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="25"/>
                            <RowDefinition Height="35*"/>
