<Window x:Name="windie" x:Class="AutoPANEL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoPANEL"
         mc:Ignorable="d"
        KeyDown="KeyDownRead" 
        KeyUp="KeyUpRead" 
        Title="AutoPANEL Beta V1.01" Height="600" Width="800
        " MinHeight="600" MinWidth="800" MaxHeight="2160" MaxWidth="3840" ScrollViewer.HorizontalScrollBarVisibility="Visible">
    <Grid OpacityMask="Black" Background="#FFDAD4D4" Margin="0,0,0,5">
        <Grid.RowDefinitions>
            <RowDefinition Height="82*"/>

        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="230"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <TextBox x:Name="xBox" Width="54" Height="20" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="23,30,485,0" RenderTransformOrigin="0.504,0.608" Visibility="Hidden" Grid.Column="1" />
        <TextBlock x:Name="xLabel" TextWrapping="Wrap" Text="X (Meters):" HorizontalAlignment="Center" Margin="164,30,8,0" Height="20" Width="58" VerticalAlignment="Top" Visibility="Hidden"/>

        <TextBox x:Name="yBox" Width="60" Height="20" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="139,30,363,0" Visibility="Hidden" Grid.Column="1" >
        </TextBox>
        <TextBlock x:Name="yLabel" TextWrapping="Wrap" Text="Y (Meters):" HorizontalAlignment="Center" Margin="95,30,409,0" Height="20" Width="58" VerticalAlignment="Top" Visibility="Hidden" Grid.Column="1"/>
        <Button x:Name="button" HorizontalAlignment="Left" Margin="153,57,0,0" VerticalAlignment="Top" Width="60" Click="PrintPDF_Click" Height="40">

            <TextBlock TextWrapping="Wrap" Text="PRINT PDF" TextAlignment="Center"/>
        </Button>
        <TextBox x:Name="percentBox" HorizontalAlignment="Left" Height="35" Margin="13,121,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="200" RenderTransformOrigin="0.84,-12.817" TextChanged="PercentBox_TextChanged" FontSize="24" TextAlignment="Center" Visibility="Hidden"/>
        <Button x:Name="saveButton" Content="SAVE" HorizontalAlignment="Left" Margin="83,17,0,0" VerticalAlignment="Top" Width="60" Click="SaveButton_Click_1" RenderTransformOrigin="2.539,3.125" Height="26"/>
        <Button x:Name="load" Content="LOAD" HorizontalAlignment="Left" Margin="153,17,0,0" VerticalAlignment="Top" Width="60" Click="Load_Click_1" RenderTransformOrigin="0.499,1.583" Height="26"/>
        <Button x:Name="New" Content="NEW" HorizontalAlignment="Left" Margin="13,17,0,0" VerticalAlignment="Top" Width="60" Click="New_Click_1" RenderTransformOrigin="0.499,1.583" Height="26"/>
        <Button x:Name="button_Copy"  HorizontalAlignment="Left" Margin="83,57,0,0" VerticalAlignment="Top" Width="60" Click="ExportImage_click" Height="40" RenderTransformOrigin="1.008,0.442">
            <TextBlock TextWrapping="Wrap" Text="EXPORT IMAGE" TextAlignment="Center"/>
        </Button>


        <TabControl x:Name="tabby1" Margin="10,50,21,26" SelectionChanged="Tabby1_SelectionChanged" Grid.Column="1" Grid.RowSpan="2">
            <TabItem Header="VIEWER" Margin="-2,0" HorizontalAlignment="Left" Width="52">
                <Grid Margin="10">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="279*"/>
                        <ColumnDefinition Width="227*"/>
                    </Grid.ColumnDefinitions>



                    <Slider Grid.Column="0" Orientation="Vertical" 

           HorizontalAlignment="Left" Minimum="0.5" Maximum="50" x:Name="slider"/>
                    <ScrollViewer Name="scrollie" 

              VerticalScrollBarVisibility="Visible" 

              HorizontalScrollBarVisibility="Visible" Margin="20,0,0,0" Grid.ColumnSpan="2">

                        <Grid Name="grid" RenderTransformOrigin="0.5,0.5" VerticalAlignment="Top" HorizontalAlignment="Left" MinWidth="460" MinHeight="460" Width="460" Height="460">


                            <Grid.LayoutTransform>
                                <TransformGroup>
                                    <ScaleTransform x:Name="scaleTransform"/>
                                </TransformGroup>
                            </Grid.LayoutTransform>
                            <Viewbox x:Name="viewy"  HorizontalAlignment="Left" VerticalAlignment="Top" Grid.RowSpan="5">
                                <Canvas x:Name="TheCanvas" MouseMove="TheCanvas_MouseMove" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0" MouseLeftButtonDown="TheCanvas_MouseLeftButtonDown" MinWidth="450" MinHeight="450" Cursor="Cross"  >
                                    <Canvas.LayoutTransform>
                                        <ScaleTransform x:Name="scaleT" ScaleX="1" ScaleY="-1" CenterX=".5" CenterY=".5" />
                                    </Canvas.LayoutTransform>
                                    <Image x:Name="ImageViewer1"  HorizontalAlignment="Left" VerticalAlignment="Bottom" Canvas.Top="0" Width="1000"  RenderTransformOrigin="0.5,0.5" Opacity="1" >
                                        <Image.RenderTransform>
                                            <TransformGroup>
                                                <ScaleTransform ScaleY="-1" ScaleX="1"/>
                                                <SkewTransform AngleY="0" AngleX="0"/>
                                                <RotateTransform Angle="0"/>
                                                <TranslateTransform/>
                                            </TransformGroup>
                                        </Image.RenderTransform>
                                    </Image>
                                </Canvas>
                            </Viewbox>
                        </Grid>

                    </ScrollViewer>

                </Grid>

            </TabItem>
            <TabItem x:Name="tabby" Header="ARRAY SETTINGS">
                <Grid Background="#FFE5E5E5" MinWidth="450" MinHeight="400.2">

                    <Grid.RowDefinitions>
                        <RowDefinition Height="34"/>

                        <RowDefinition Height="343"/>
                        <RowDefinition Height="80*"/>
