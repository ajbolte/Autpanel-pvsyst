                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" Grid.Row="2" Grid.ColumnSpan="2" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center"><Run Text="Gap Between Panels "/><Run Text="Down"/><Run Text=" (mm"/><Run Text=")"/></TextBlock>
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" Grid.Row="6" VerticalAlignment="Center" HorizontalAlignment="Center"><Run Text="Walkway Across Siz"/><Run Text="e (mm)"/></TextBlock>
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" Grid.Row="6" Grid.Column="3" VerticalAlignment="Center" HorizontalAlignment="Center"><Run Text="Walkway "/><Run Text="Down"/><Run Text=" Siz"/><Run Text="e (mm)"/></TextBlock>
