                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" Grid.Row="4" Grid.Column="3" VerticalAlignment="Center" HorizontalAlignment="Center"><Run Text="Consecutive Panels "/><LineBreak/><Run Text="between Walkways"/><Run Text=" "/><Run Text="Down"/></TextBlock>
                        <ComboBox x:Name="PanelTypes" HorizontalAlignment="Center" Margin="31,0.6,29.2,0" VerticalAlignment="Top" ItemsSource="{Binding}" SelectionChanged="PanelTypes_SelectionChanged" Grid.Row="13" MinWidth="170" DropDownClosed="OrientationBox_DropDownClosed"/>
                        <ComboBox x:Name="OrientationBox" Grid.Column="3" HorizontalAlignment="Center" Margin="32,0,30,9" VerticalAlignment="Center" SelectionChanged="PanelTypes_SelectionChanged" Grid.Row="13" RenderTransformOrigin="0.499,1.17" MinWidth="170" DropDownClosed="OrientationBox_DropDownClosed_1"/>
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" RenderTransformOrigin="2.946,0.95" Grid.Row="12" VerticalAlignment="Center" Margin="10,0,10,2" HorizontalAlignment="Center"><Run Text="Panel Type"/></TextBlock>
